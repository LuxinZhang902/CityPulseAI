# SnowLeopard.ai API Key
# Get your API key from: https://snowleopard.ai
SNOWLEOPARD_API_KEY=your_snowleopard_api_key_here

# SnowLeopard Datafile ID (upload citypulse_for_playground.db to get this)
SNOWLEOPARD_DATAFILE_ID=your_datafile_id_here

# Enable real-time data sync on startup (true/false)
# When true, fetches fresh data from SF Open Data APIs at startup
SYNC_REALTIME_DATA=false

# Google Maps API Key (for frontend)
# Get your API key from: https://console.cloud.google.com/google/maps-apis
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Backend API URL (for frontend)
REACT_APP_API_URL=http://localhost:8000
